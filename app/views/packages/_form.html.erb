<%= form_with(model: [ @courier, @courier.packages.build ], local: true) do |form| %>
  <p>
    <%= form.label :tracking_number %><br>
    <%= form.text_field :tracking_number %>
  </p>
  <p>
    <%= form.label :delivery_status %><br>
    <%= form.check_box :delivery_status %>      
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>